<!DOCTYPE html>
<html>
<head>
  <title>WebPlayBot</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div id="white"></div>
<div id="header">
  <ul>
    <li id="menu">
    <span><a id="menu_button">Menu</a></span>
    <span class="menu_toggleable"><a href="/musics">Home</a></span> 
    <span class="menu_toggleable"><a href="/channels">Channels</a></span> 
    <span class="menu_toggleable"><a href="/users">Users</a></span> 
    <span class="menu_toggleable"><a href="/tags">Tags</a></span> 
    </li>
    <!-- </ul>
    <div id="player_position"><div id="player_block"></div></div> -->
    <li>
    <span id="player_button"></span>
    <span id="player_position"><span id="player_block"></span></span>
    <span id="next_button"></span>
    </li>
  </ul>
</div>
<%= yield %>

</body>
<%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
<script src="https://www.youtube.com/iframe_api"></script>
<script src="https://connect.soundcloud.com/sdk-2.0.0.js"></script>
<script src="https://w.soundcloud.com/player/api.js"></script>

</html>
